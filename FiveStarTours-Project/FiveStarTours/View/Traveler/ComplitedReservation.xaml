<Window x:Class="FiveStarTours.View.Traveler.ComplitedReservationxaml"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FiveStarTours.View.Traveler"
        mc:Ignorable="d"
        Title="ComplitedReservationxaml" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black"/>
                <GradientStop Color="#FFF39494" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Label Content="Finaly you complited your reservation" HorizontalAlignment="Center" Margin="0,104,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Bold" Background="#FFDFC8C8"/>
        <Label Content="We hope you'll have great time here" HorizontalAlignment="Center" Margin="0,165,0,0" VerticalAlignment="Top" FontSize="22" FontWeight="Bold" Background="#FFDFC8C8"/>
        <Button Content="Go to the begining" HorizontalAlignment="Left" Margin="208,309,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold" Width="366" Height="69" BorderThickness="7,7,7,7" Background="#FFEBA2A2" BorderBrush="#FF131111" Click="Button_Click"/>

    </Grid>
</Window>
