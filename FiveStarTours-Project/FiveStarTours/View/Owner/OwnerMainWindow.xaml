<Window x:Class="FiveStarTours.View.OwnerMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FiveStarTours.View"
        mc:Ignorable="d"
        Title="OwnerMainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Register new accommodation" 
                HorizontalAlignment="Left" 
                Margin="92,298,0,0" 
                VerticalAlignment="Top" 
                Height="47" Width="213" 
                Click="AddAccommodationButton_Click"/>

        <Button Content="Guest rating" 
                HorizontalAlignment="Left" 
                Margin="458,298,0,0" 
                VerticalAlignment="Top" 
                Height="47" Width="213" 
                Click="GuestRatingButton_Click"/>
    </Grid>
</Window>
